PeerOrgs:
  {% for o in orgs %}
  - Name: {{ o['name'] }}
    Domain: {{ o['name'] }}.{{ domain }}
    EnableNodeOUs: true
    Template:
      Count: {{ o['peers'] }}
      SANS:
        - localhost
    Users:
      Count: {{ o['peers'] }}
  {% endfor %}
